    "def mkframe(country):\n",
    "    df = pd.DataFrame({ 'infected' : infected.loc[country] ,\n",
    "                        'recovered' : recovered.loc[country],\n",
    "                        'deaths' : deaths.loc[country]})\n",
    "    df.index = pd.to_datetime(df.index)\n",
    "    return df\n",
    "\n",
    "df = mkframe('US')\n",
    "df"
   ]
  },
